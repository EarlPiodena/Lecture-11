#include <iostream>
#include <string>
using namespace std;

int main()
{
	int a = 1, b = 1;
	while (a <= 5)
	{
		b = 1;
		while (b <= a)
		{
			cout << "*"; 
			b++;
		}
		cout << endl;
		a++;
	}

	int x = 1, y = 1;
	while (x <= 5)
	{
		y = x;
		while (y <= 5)
		{
			cout << "*"; 
			y++;
		}
		cout << endl;
		x++;
	}
}
