#include <iostream>
#include <string>
using namespace std;

int main()
{
    cout << "Enter the number that you want to factor: " << endl;
    double x, fact = 1;
    cin >> x;

    for (int y = x; y > 0; y--)
    {
        fact = y * fact;
    }
    cout << "The factorial of " << x << " is: " << fact << endl;
}
